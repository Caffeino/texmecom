{
	"name": "@texmecom/api-gateway-e2e",
	"version": "0.0.1",
	"private": true,
	"nx": {
		"implicitDependencies": [
			"@texmecom/api-gateway"
		],
		"targets": {
			"e2e": {
				"executor": "@nx/jest:jest",
				"outputs": [
					"{projectRoot}/test-output/jest/coverage"
				],
				"options": {
					"jestConfig": "apps/api-gateway-e2e/jest.config.ts",
					"passWithNoTests": true
				},
				"dependsOn": [
					"@texmecom/api-gateway:build",
					"@texmecom/api-gateway:serve"
				]
			}
		}
	}
}
