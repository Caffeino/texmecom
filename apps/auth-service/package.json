{
	"name": "@texmecom/auth-service",
	"version": "0.0.1",
	"private": true,
	"nx": {
		"targets": {
			"serve": {
				"continuous": true,
				"executor": "@nx/js:node",
				"defaultConfiguration": "development",
				"dependsOn": [
					"build"
				],
				"options": {
					"buildTarget": "@texmecom/auth-service:build",
					"runBuildTargetDependencies": false
				},
				"configurations": {
					"development": {
						"buildTarget": "@texmecom/auth-service:build:development"
					},
					"production": {
						"buildTarget": "@texmecom/auth-service:build:production"
					}
				}
			},
			"test": {
				"options": {
					"passWithNoTests": true
				}
			}
		}
	}
}
